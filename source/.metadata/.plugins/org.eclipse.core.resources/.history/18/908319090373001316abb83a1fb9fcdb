typedef struct s_servo {
	char *DDR;
	char *PORT;
	char pinmask;
	char value;
} t_servo, *p_servo;

t_servo[] servos = {
		{&DDRB, PORTB, (1<<PB0), 0},
		{&DDRB, PORTD, (1<<PD5), 0},
		{&DDRB, PORTD, (1<<PD6), 0},
		{&DDRB, PORTD, (1<<PD7), 0},

		{&DDRB, PORTB, (1<<PB1), 0},
		{&DDRB, PORTB, (1<<PB2), 0},
		{&DDRB, PORTB, (1<<PB3), 0},
		{&DDRB, PORTB, (1<<PB4), 0},

};
